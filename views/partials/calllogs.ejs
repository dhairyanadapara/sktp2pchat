<h2>Call Logs</h2>
<div class="table-responsive">

    <table class="table table-striped table-sm">
        <thead>
            <tr>
                <th>cid</th>
                <th>rid</th>
                <th>Date</th>
                <th>Header</th>
                <th>startTime</th>
            </tr>
        </thead>
        <%  Call.forEach(function (calls) {%>
            <span data-feather="calls">
                <tbody>
                    <tr>
                        <td>
                            <%=calls.rid%>
                        </td>
                        <td>
                            <%=calls.cid%>
                        </td>
                        <td>
                            <%=calls.date%>
                        </td>
                        <td>
                            <%=calls.startTime%>
                        </td>
                        <td>
                            <%=calls%>
                        </td>
                    </tr>
                </tbody>
            </span>
            <%});%>
    </table>
</div>